{
  "name": "redux-reset-store",
  "version": "1.0.1",
  "description": "Higher Order Reducer for resetting multiple parts of your state tree",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel ./src --ignore test --out-dir ./dist",
    "test": "mocha --require babel-core/register ./test",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1",
    "redux": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhiaiyer91/redux-reset.git"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  },
  "keywords": [
    "redux",
    "react",
    "Javascript",
    "reset"
  ],
  "author": "Abhi Aiyer",
  "license": "ISC"
}
